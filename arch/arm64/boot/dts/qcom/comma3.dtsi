  qcom,platform-reset-gpio = <&tlmm 6 0>;
  //qcom,panel-errfg-gpio = <&tlmm 135 0>

  qcom,mdss-dsi-t-clk-post = <0x0e>;
	qcom,mdss-dsi-t-clk-pre = <0x31>;
